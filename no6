#a
n = 100
m = 50
std = 8

set.seed(100)
data <- rnorm(100,50,8)

rata_rata <- mean(data)
rata_rata
x1 <- floor(mean(data))
x1
x2 <- round(mean(data))
if (x1 == x2){
  x2=x2+1
  print(x2)
}else{
  print(x2)
}

z_scores <- (data - mean(data) / sd(data))

plot(z_scores, type = "o", col="orange")

