#data <- c(1,2,4,2,6,3,10,11,5,3,6,85)
data <- rnorm(100,50,8)

rata_rata <- mean(data)
rata_rata
x1 <- floor(mean(data))
x1
x2 <- round(mean(data))
x2

z_scores <- (data - mean(data) / sd(data))

plot(z_scores, type = "o", col="orange")

